{
    "name": "phpdocumentor/guides-cli",
    "description": "Allows you to run phpDocumentor Guides as a stand alone application as console command",
    "type": "project",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Guides\\Cli\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jaapio",
            "email": "jaap@phpdoc.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^2.9",
        "phpdocumentor/guides": "self.version",
        "phpdocumentor/guides-restructured-text": "self.version",
        "symfony/config": "^5.4 || ^6.3",
        "symfony/console": "^5.4 || ^6.3",
        "symfony/dependency-injection": "^5.4 || ^6.3",
        "symfony/event-dispatcher": "^5.4 || ^6.3"
    },
    "bin": [
        "bin/guides"
    ]
}
